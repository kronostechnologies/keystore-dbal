{
    "name": "kronos/keystore-dbal",
    "description": "DBAL implementation of kronos/keystore repository",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Kronos\\Keystore\\Repository\\DBAL\\": "src/DBAL/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kronos\\Tests\\Keystore\\Repository\\DBAL\\": "tests/DBAL/"
        }
    },
    "require": {
        "kronos/keystore": "^1.0.0",
        "doctrine/dbal": "^3.4",
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "11.0.1",
        "vimeo/psalm": "5.20.0",
        "squizlabs/php_codesniffer": "3.8.1",
        "dealerdirect/phpcodesniffer-composer-installer": "1.0.0",
        "kronostechnologies/php-coding-standard": "2.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Breton",
            "email": "jbreton@kronostechnologies.com"
        }
    ],
    "config": {
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
